<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewProjects">

    <LinearLayout
        android:id="@+id/GetDataLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="128dp"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/studentIDViewText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/titleViewText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/descriptionViewText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/yearViewText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/nameFirstViewText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/nameLastViewText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/view_projects"
            android:textSize="18sp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>